<form method="post" action="/songs/edit">
  Name <input type="text" name="song[name]" id="Name"></input>
  <input type="text" name="song[artist][name]" id="Artist Name"></input>
  <% Genre.all.each do |genre| %>
    <input id="<%= genre.name %>" type="checkbox" name="genres[]" value="<%= genre.id %>"><%=genre.name%></input>
  <% end %>
  <input type="submit" value="Save">
</form>
